<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/ll_mainNotification"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.fragments.setting.view.EmailFragment">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:padding="@dimen/_5sdp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_5sdp"
                android:layout_weight="1"
                android:fontFamily="@font/brandon_reg"
                android:padding="@dimen/_5sdp"
                android:text="@string/follow_unfollow"
                android:textColor="@color/white"
                android:textSize="@dimen/_15ssp" />


            <com.google.android.material.switchmaterial.SwitchMaterial
                android:id="@+id/switch_follow_unfollow"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@null"
                android:button="@drawable/custom_notificationbutton"
                android:padding="@dimen/_5sdp"
                android:thumb="@null"
                app:track="@null" />
        </LinearLayout>

        <com.google.android.material.internal.BaselineLayout
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/baseLineGray" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:padding="@dimen/_5sdp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_5sdp"
                android:layout_weight="1"
                android:fontFamily="@font/brandon_reg"
                android:padding="@dimen/_5sdp"
                android:text="@string/connect_disconnect"
                android:textColor="@color/white"
                android:textSize="@dimen/_15ssp" />


            <com.google.android.material.switchmaterial.SwitchMaterial
                android:id="@+id/switch_connectDisconnect"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@null"
                android:button="@drawable/custom_notificationbutton"
                android:padding="@dimen/_5sdp"
                android:thumb="@null"
                app:track="@null" />
        </LinearLayout>

        <com.google.android.material.internal.BaselineLayout
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/baseLineGray" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:padding="@dimen/_5sdp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_5sdp"
                android:layout_weight="1"
                android:fontFamily="@font/brandon_reg"
                android:padding="@dimen/_5sdp"
                android:text="@string/like"
                android:textColor="@color/white"
                android:textSize="@dimen/_15ssp" />


            <com.google.android.material.switchmaterial.SwitchMaterial
                android:id="@+id/switch_like"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@null"
                android:button="@drawable/custom_notificationbutton"
                android:padding="@dimen/_5sdp"
                android:thumb="@null"
                app:track="@null" />
        </LinearLayout>

        <com.google.android.material.internal.BaselineLayout
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/baseLineGray" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:padding="@dimen/_5sdp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_5sdp"
                android:layout_weight="1"
                android:fontFamily="@font/brandon_reg"
                android:padding="@dimen/_5sdp"
                android:text="@string/vote"
                android:textColor="@color/white"
                android:textSize="@dimen/_15ssp" />


            <com.google.android.material.switchmaterial.SwitchMaterial
                android:id="@+id/switch_vote"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@null"
                android:button="@drawable/custom_notificationbutton"
                android:padding="@dimen/_5sdp"
                android:thumb="@null"
                app:track="@null" />
        </LinearLayout>

        <com.google.android.material.internal.BaselineLayout
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/baseLineGray" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:padding="@dimen/_5sdp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_5sdp"
                android:layout_weight="1"
                android:fontFamily="@font/brandon_reg"
                android:padding="@dimen/_5sdp"
                android:text="@string/comment"
                android:textColor="@color/white"
                android:textSize="@dimen/_15ssp" />


            <com.google.android.material.switchmaterial.SwitchMaterial
                android:id="@+id/switch_comment"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@null"
                android:button="@drawable/custom_notificationbutton"
                android:padding="@dimen/_5sdp"
                android:thumb="@null"
                app:track="@null" />
        </LinearLayout>


    </LinearLayout>

</LinearLayout>